  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Program",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}\\.ops\\headliner\\index.js",
      "args": ["."],
      "cwd": "${workspaceFolder}"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Program 2",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}\\.ops\\headliner\\index.js",
      "args": ["test.md"],
      "cwd": "${workspaceFolder}/.ops/headliner"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "HBS CLI",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}\\.ops\\hbscli2\\index.js",
      "args": ["headlines.json", "allrules.md.hbs", "output.md"],
      "cwd": "${workspaceFolder}/.ops/hbscli2"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Glossary",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}\\.ops\\glossary\\index.js",
      "args": ["."],
      "cwd": "${workspaceFolder}/.ops/glossary"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Statistics",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}\\.ops\\statistics\\index.js",
      "args": ["../../guidelines/de-DE/"],
      "cwd": "${workspaceFolder}/.ops/statistics"
    }
  ]
}